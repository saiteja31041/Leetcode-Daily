class Solution {
    public void merge(int[] num1, int m, int[] num2, int n) {
        int midx = m-1;
        int nidx = n-1;
        int index = m+n-1;
        while(nidx>=0){
            if(midx >= 0 && num1[midx]>=num2[nidx]){
                num1[index] = num1[midx];
                index--;
                midx--;
            }
            else{
                num1[index] = num2[nidx];
                index--;
                nidx--;
            }
        }
    }
}
